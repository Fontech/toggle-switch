{
    "name": "fontech/toggle-switch",
    "description": "A Laravel Nova toggle switch field",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0"
    },
    "autoload": {
        "psr-4": {
            "Fontech\\ToggleSwitch\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Fontech\\ToggleSwitch\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
